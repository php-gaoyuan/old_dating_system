.home_scroll{ width:100%; height: auto; position: absolute; left: 0px; bottom:4%; animation: myfirst 1s;
    -moz-animation: myfirst 1s;	/* Firefox */
    -webkit-animation: myfirst 1s;	/* Safari 和 Chrome */
    -o-animation: myfirst 1s;	/* Opera */}
@keyframes myfirst
{
0%   {bottom:0px;}
20%  {bottom:30px;}
40%  {bottom:0px;}
60%  {bottom:30px;}
80%  {bottom:0px;}
100% {bottom:30px;}
}

@-moz-keyframes myfirst /* Firefox */
{
0%   {margin:60px auto}
20%  {margin:0px auto}
40%  {margin:40px auto}
60%  {margin:0px auto}
80%  {margin:20px auto}
100% {margin:0px auto}
}

@-webkit-keyframes myfirst /* Safari 和 Chrome */
{
0%   {bottom:120px;}
30%  {bottom:30px;}
60%  {bottom:100px}
80%  {bottom:30px}
100% {bottom:32px;}
}

@-o-keyframes myfirst /* Opera */
{
0%   {margin:60px auto}
20%  {margin:0px auto}
40%  {margin:40px auto}
60%  {margin:0px auto}
80%  {margin:20px auto}
100% {margin:0px auto}
}
